{
  "name": "nextjs-testing",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "test": "jest --env=jsdom --verbose"
  },
  "dependencies": {
    "autoprefixer": "^10.4.19",
    "axios": "^1.6.8",
    "jest-environment-jsdom": "^29.7.0",
    "msw": "^2.3.0",
    "next": "^12.0.7",
    "next-page-tester": "^0.33.0",
    "postcss": "^8.2.4",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "setimmediate": "^1.0.5",
    "swr": "^2.2.5",
    "tailwindcss": "^3.4.3"
  },
  "prettier": {
    "singleQuote": true,
    "semi": false
  },
  "jest": {
    "testPathIgnorePatterns": [
      "<rootDir>/.next/",
      "<rootDir>/node_modules/"
    ],
    "moduleNameMapper": {
      "\\.(css)$": "<rootDir>/node_modules/jest-css-modules"
    },
    "setupFilesAfterEnv": [
      "<rootDir>/jest.setup.js"
    ]
  },
  "devDependencies": {
    "@testing-library/dom": "^10.1.0",
    "@testing-library/jest-dom": "^6.4.5",
    "@testing-library/react": "^15.0.7",
    "@testing-library/user-event": "^14.5.2",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.12.12",
    "@types/react": "^18.3.2",
    "babel-jest": "^29.7.0",
    "jest": "^29.7.0",
    "jest-css-modules": "^2.1.0",
    "node-fetch": "^3.3.2",
    "typescript": "^5.4.5"
  }
}
